{
  "name": "appspider-validate-chrome-plugin",
  "description": "Chrome plugin used to enable the validate button in AppSpider reports.",
  "authors": [
    "Nonico Bugash"
  ],
  "license": "MIT",
  "keywords": [
    "chrome-plugin",
    "base",
    "chrome",
    "plugin"
  ],
  "moduleType": [
    "node"
  ],
  "homepage": "https://github.com/nbugash-r7/appspider-validate-chrome-plugin#readme",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "ui-base-styles": "git@github.com:rapid7/ui-base-styles.git",
    "jquery": "^2.2.1",
    "underscore": "^1.8.3",
    "bootstrap-toggle": "^2.2.1",
    "angular": "^1.5.0",
    "dot-object": "^1.4.1",
    "bootstrap": "^3.3.6",
    "angular-bootstrap": "^1.2.4"
  },
  "overrides": {
    "dot-object": {
      "main": [
        "dist/dot-object.min.js"
      ]
    },
    "jquery": {
      "main": [
        "dist/jquery.min.js"
      ]
    },
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/_bootstrap.scss",
        "assets/javascripts/bootstrap.min.js"
      ]
    },
    "underscore": {
      "main": [
        "underscore-min.js"
      ]
    },
    "angular": {
      "main": [
        "angular.min.js"
      ]
    },
    "ui-base-styles": {
      "main": [
        "dist/styles/base.css",
        "dist/javascripts/bootstrap.js"
      ]
    },
    "bootstrap": {
      "main": [
        "less/bootstrap.less",
        "dist/js/bootstrap.min.js"
      ]
    }
  }
}
